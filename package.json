{
  "name": "passbolt-mad",
  "version": "2.0.0",
  "author": "Passbolt SARL",
  "license": "AGPL-3.0",
  "description": "Passbolt javascript application library for the open source password manager for teams",
  "main": "src/mad",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:passbolt/passbolt-mad.git"
  },
  "dependencies": {
    "steal": "~0.10.4",
    "can": "~2.2.6",
    "jquery": "~2.1.4",
    "underscore": "~1.8.3",
    "xregexp": "~3.0.0"
  },
  "devDependencies": {
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-shell": "^1.3.0",
    "documentjs": "^0.4.4",
    "steal-tools": "^0.16.5",
    "mocha": "^2.2.5",
    "steal-mocha": "^0.0.2",
    "chai": "^3.0.0",
    "chai-jq": "^0.0.9",
    "sinon": "^1.16.1"
  },
  "scripts": {
    "build": "steal-tools",
    "patch": "npm version patch -m \"patch %s\"",
    "minor": "npm version minor -m \"minor %s\"",
